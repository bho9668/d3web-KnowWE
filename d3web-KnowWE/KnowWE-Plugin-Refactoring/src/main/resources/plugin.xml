<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="KnowWE-Plugin-Refactoring" version="0.0.1">
	
	<requires>
		<import plugin-id="KnowWEExtensionPoints" />
	</requires>
	
	<runtime>
		<library id="de.d3web" path="." type="code">
			<export prefix="*" />
		</library>
		<library id="webapp" path="webapp/" type="resources">
			<export prefix="*" />
		</library>
	</runtime>
	
	<extension plugin-id="KnowWEExtensionPoints" point-id="KnowWEAction"
		id="RefactoringAction">
		<parameter id="class"
			value="de.d3web.we.refactoring.action.RefactoringAction" />
		<parameter id="name" value="RefactoringAction" />
		<parameter id="description" value="KnowWEAction RefactoringAction" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
	</extension>
	
	<extension plugin-id="KnowWEExtensionPoints" point-id="TagHandler"
		id="RefactoringTagHandler">
		<parameter id="class"
			value="de.d3web.we.refactoring.RefactoringTagHandler" />
		<parameter id="name" value="RefactoringTagHandler" />
		<parameter id="description" value="TagHandler RefactoringTagHandler" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
	</extension>
	
	<extension plugin-id="KnowWEExtensionPoints" point-id="KnowWEObjectType"
		id="Refactoring">
		<parameter id="class"
			value="de.d3web.we.refactoring.Refactoring" />
		<parameter id="name" value="Refactoring" />
		<parameter id="description" value="KnowWEObjectType Refactoring" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
		<parameter id="scope" value="root" />
	</extension>
	
</plugin>