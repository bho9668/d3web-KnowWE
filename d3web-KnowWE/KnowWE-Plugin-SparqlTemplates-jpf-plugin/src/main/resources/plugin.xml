<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="KnowWE-Plugin-SparqlTemplates" version="0.0.1">
	
	<requires>
	<import plugin-id="KnowWEExtensionPoints" />
	</requires>
	
	<runtime>
		<library id="de.d3web" path="." type="code">
			<export prefix="*" />
		</library>
		<library id="webapp" path="webapp/" type="resources">
			<export prefix="*" />
		</library>
	</runtime>
	
	
	
	<extension plugin-id="KnowWEExtensionPoints" point-id="KnowWEObjectType"
		id="GroovySparqlRenderer">
		<parameter id="class"
			value="de.d3web.we.kdom.sparql.groovy.GroovySparqlRenderer" />
		<parameter id="name" value="GroovySparqlRenderer" />
		<parameter id="description" value="KnowWEObjectType GroovySparqlRenderer" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
		<parameter id="scope" value="root" />
	</extension>
	
</plugin>
