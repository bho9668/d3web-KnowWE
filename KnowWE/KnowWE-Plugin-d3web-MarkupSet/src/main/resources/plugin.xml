<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="KnowWE-Plugin-d3web-MarkupSet" version="0.0.1">
	
	<requires>
		<import plugin-id="KnowWEExtensionPoints" />
	</requires>
	
	<runtime>
		<library id="de.d3web" path="." type="code">
			<export prefix="*" />
		</library>
	</runtime>
	
	<extension plugin-id="KnowWEExtensionPoints" point-id="Type"
		id="QuestionTreeRootType">
		<parameter id="class"
			value="de.d3web.we.kdom.questionTree.QuestionTreeRootType" />
		<parameter id="name" value="QuestionTreeRootType" />
		<parameter id="description" value="Type QuestionTreeRootType" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="4.55" />
		<parameter id="scope" value="root" />
		<parameter id="script" value="KnowWE-Plugin-d3web-basic.js" />
	</extension>
	

	<extension plugin-id="KnowWEExtensionPoints" point-id="Type"
		id="SolutionsDashTree">
		<parameter id="class"
			value="de.d3web.we.kdom.solutionTree.SolutionTreeMarkup" />
		<parameter id="name" value="SolutionTreeMarkup" />
		<parameter id="description" value="Type SolutionTreeMarkup" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="4.5" />
		<parameter id="scope" value="root" />
		<parameter id="script" value="KnowWE-Plugin-d3web-basic.js" />
	</extension>
	
	<extension plugin-id="KnowWEExtensionPoints" point-id="Type"
		id="QuestionnairesDashTree">
		<parameter id="class"
			value="de.d3web.we.kdom.questionnaireTree.QuestionnaireTreeMarkup" />
		<parameter id="name" value="QuestionnaireTreeMarkup" />
		<parameter id="description" value="Type QuestionnaireTreeMarkup" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="4.5" />
		<parameter id="scope" value="root" />
		<parameter id="script" value="KnowWE-Plugin-d3web-basic.js" />
	</extension>
	
	<extension plugin-id="KnowWEExtensionPoints" point-id="Type"
		id="CoveringListMarkup">
		<parameter id="class"
			value="de.d3web.we.kdom.xcl.list.CoveringListMarkup" />
		<parameter id="name" value="CoveringListMarkup" />
		<parameter id="description" value="Type CoveringListMarkup" />
