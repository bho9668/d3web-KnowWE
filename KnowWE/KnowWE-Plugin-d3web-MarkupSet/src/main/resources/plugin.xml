		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5.0" />
		<parameter id="scope" value="root" />
		<parameter id="script" value="KnowWE-Plugin-d3web-basic.js" />
	</extension>
	
	<extension plugin-id="KnowWEExtensionPoints" point-id="Type"
		id="PropertyMarkup">
		<parameter id="class"
			value="de.knowwe.d3web.property.PropertyMarkup" />
		<parameter id="name" value="PropertyMarkup" />
		<parameter id="description" value="Markup to add properties to IDObjects (optional language and country specific)" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="7.0" />
		<parameter id="scope" value="root" />
		<parameter id="script" value="KnowWE-Plugin-d3web-basic.js" />
	</extension>
	
	<extension plugin-id="KnowWEExtensionPoints" point-id="Type"
		id="InitValueType">
		<parameter id="class"
			value="de.knowwe.d3web.initValue.InitValueType" />
