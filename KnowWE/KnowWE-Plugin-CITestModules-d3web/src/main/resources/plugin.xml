<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="KnowWE-Plugin-CITestModules-d3web" version="0.0.1">
	
	<requires>
		<import plugin-id="KnowWEExtensionPoints" />
		<import plugin-id="d3web-Plugin-TestingFramework" />
	</requires>
	
	<runtime>
		<library id="de.d3web" path="." type="code">
			<export prefix="*" />
		</library>
		<library id="webapp" path="webapp/" type="resources">
			<export prefix="*" />
		</library>
	</runtime>


	<extension plugin-id="d3web-Plugin-TestingFramework" point-id="Test"
		id="EmptyQuestionnaireTest">
		<parameter id="class"
			value="de.d3web.we.ci4ke.testmodules.EmptyQuestionnaireTest" />
		<parameter id="name" value="EmptyQuestionnaireTest" />
		<parameter id="description" value="CITest EmptyQuestionnaireTest" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
	</extension>

	<extension plugin-id="d3web-Plugin-TestingFramework" point-id="Test"
		id="TestsuiteRunnerTest">
		<parameter id="class"
			value="de.d3web.we.ci4ke.testmodules.TestsuiteRunnerTest" />
		<parameter id="name" value="TestsuiteRunner" />
		<parameter id="description" value="CITest TestsuiteRunner" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
	</extension>
	
		<extension plugin-id="d3web-Plugin-TestingFramework" point-id="Test"
		id="TestCaseTest">
		<parameter id="class"
			value="de.d3web.we.ci4ke.testmodules.TestCaseTest" />
		<parameter id="name" value="TestCaseTest" />
		<parameter id="description" value="CITest TestCaseTest" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
	</extension>
	
	<extension plugin-id="d3web-Plugin-TestingFramework" point-id="TestObjectProvider"
		id="KnowledgeBaseTestObjectProvider">
		<parameter id="class"
			value="de.d3web.we.ci4ke.KnowledgeBaseTestObjectProvider" />
		<parameter id="name" value="KnowledgeBaseTestObjectProvider" />
		<parameter id="description" value="KnowledgeBaseTestObjectProvider" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
	</extension>
	
		<extension plugin-id="d3web-Plugin-TestingFramework" point-id="TestObjectProvider"
		id="TestCaseTestObjectProvider">
		<parameter id="class"
			value="de.d3web.we.ci4ke.TestCaseTestObjectProvider" />
		<parameter id="name" value="TestCaseTestObjectProvider" />
		<parameter id="description" value="TestCaseTestObjectProvider" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
	</extension>
	
</plugin>
