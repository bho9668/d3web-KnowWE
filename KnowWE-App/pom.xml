	<parent>
		<artifactId>KnowWE-Essentials</artifactId>
		<groupId>de.uniwue.d3web</groupId>
		<version>1.0-SNAPSHOT</version>
		<relativePath>../KnowWE-Essentials</relativePath>
	</parent>

	<artifactId>KnowWE-App</artifactId>
	<version>1.0-SNAPSHOT</version>
	<packaging>war</packaging>
	<description>Maven module for the KnowWE web app. Produces a .war file.</description>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-dependency-plugin</artifactId>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-war-plugin</artifactId>
				<version>2.3-uniwue</version>
				<configuration>
					<overlays>
						<overlay>
							<groupId>de.uniwue.d3web</groupId>
							<artifactId>KnowWE-Resources</artifactId>
						</overlay>
					</overlays>
				</configuration>
				<executions>
					<execution>
						<id>prepare-war</id>
						<phase>prepare-package</phase>
						<goals>
							<goal>exploded</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>de.uniwue.maven.plugins</groupId>
				<artifactId>knowwe-versionator</artifactId>
				<executions>
					<execution>
						<id>vers</id>
						<phase>prepare-package</phase>
						<goals>
							<goal>versionate</goal>
						</goals>
						<configuration>
							<targetDir>
								${project.build.directory}/${project.artifactId}-${project.version}/WEB-INF/classes/
							</targetDir>
						</configuration>
					</execution>
					<execution>
						<id>versionate-plugin</id>
						<phase>none</phase>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>

