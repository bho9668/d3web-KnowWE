	<modelVersion>4.0.0</modelVersion>

	<parent>
		<artifactId>KnowWE</artifactId>
		<groupId>de.uniwue.d3web</groupId>
		<version>0.2-SNAPSHOT</version>
		<relativePath>.../KnowWE/pom.xml</relativePath>
	</parent>

	<groupId>de.uniwue.d3web</groupId>
	<artifactId>KnowWE-App</artifactId>
	<packaging>war</packaging>
	<name>KnowWE</name>
	<version>0.2-SNAPSHOT</version>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-dependency-plugin</artifactId>
				<executions>
					<execution>
						<id>copy-dependencies</id>
						<phase>test-compile</phase>
						<goals>
							<goal>build-classpath</goal>
						</goals>
						<configuration>
							<outputDirectory>target/dependencies</outputDirectory>
							<overWriteReleases>false</overWriteReleases>
							<overWriteSnapshots>false</overWriteSnapshots>
							<overWriteIfNewer>true</overWriteIfNewer>
							<outputFile>target/dependencies/output.txt</outputFile>
							<pathSeparator>;</pathSeparator>
						</configuration>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-war-plugin</artifactId>
				<configuration>
					<overlays>
						<overlay>
							<groupId>de.uniwue.d3web</groupId>
							<artifactId>KnowWE-Resources</artifactId>
						</overlay>
					</overlays>
				</configuration>
				<executions>
					<execution>
						<id>prepare-war</id>
						<phase>prepare-package</phase>
						<goals>
							<goal>exploded</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>de.uniwue.maven.plugins</groupId>
				<artifactId>knowwe-versionator</artifactId>
				<version>${versionatorVersion}</version>
				<executions>
					<execution>
						<id>vers</id>
						<phase>prepare-package</phase>
						<goals>
							<goal>versionate</goal>
						</goals>
						<configuration>
							<targetDir>${project.build.directory}/${pom.artifactId}-${pom.version}/WEB-INF/classes/</targetDir>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>

